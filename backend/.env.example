PORT=3000
# Choose provider: 'openai' or 'groq'
AI_PROVIDER=groq

# ============================================
# EXISTING KEYS (Keep these)
# ============================================
GROQ_API_KEY=your_groq_key_here
OPENAI_API_KEY=your_openai_key_here
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_anon_key

# ============================================
# NEW - GOOGLE OAUTH (Required for Google Sign-In)
# ============================================
# Get these from: https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Random secret for session encryption (generate a random string)
SESSION_SECRET=your_random_secret_string_min_32_chars

# ============================================
# NEW - PUSH NOTIFICATIONS (Required for Phone Alerts)
# ============================================
# These keys were generated by: npx web-push generate-vapid-keys
VAPID_PUBLIC_KEY=
VAPID_PRIVATE_KEY=

# Note:
# - Don't commit this file to Git
# - In production, use environment variables securely
# - The VAPID keys above are unique to this app - don't regenerate


